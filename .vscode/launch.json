	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python: Current File",
			"type": "python",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal",
			"justMyCode": true
		},
		{
			"name": "Python: app.py",
			"type": "python",
			"request": "launch",
			"program": "src/populate_ssm/app.py",
			"cwd": "${workspaceFolder}",
			"args": [
				"--env-file",
				"test/one.env",
				"--json-file",
				"test/one.json",
				"/myorg/test/app"
			],
			"console": "integratedTerminal",
			"justMyCode": true
		}
	]
}
